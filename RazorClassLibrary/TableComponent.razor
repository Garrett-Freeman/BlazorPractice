
<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Eamil</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var row in Data)
        {
            <tr>
                <td>@row.Id</td>
                <td>@row.FirstName</td>
                <td>@row.LastName</td>
                <td>@row.Email</td>
            </tr>
        }
    </tbody>
</table>
